{
  "name": "stencil-boilerplate",
  "version": "0.0.1",
  "description": "Stencil boilerplate for Web components",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-boilerplate/stencil-boilerplate.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "NODE_ENV=production stencil build --docs --prerender",
    "build-storybook": "build-storybook -s dist/ -o docs/",
    "clean": "node scripts/clean.js",
    "dev": "stencil build --dev --watch --serve",
    "generate": "stencil generate",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "serve": "serve www -p 5000",
    "start": "run-p dev storybook",
    "storybook": "start-storybook -p 6006 -s dist/",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "format": "prettier --write \"src/**/*.{ts,js}\""
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@stencil/core": "^2.0.0-7",
    "@stencil/eslint-plugin": "^0.3.1",
    "@stencil/postcss": "^1.0.1",
    "@storybook/addon-essentials": "^6.0.20",
    "@storybook/web-components": "^6.0.20",
    "@types/jest": "^26.0.9",
    "@types/puppeteer": "2.0.1",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.3",
    "cssnano": "^4.1.10",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react": "^7.20.5",
    "jest": "^26.2.2",
    "jest-cli": "26.0.1",
    "lit-html": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "puppeteer": "2.1.1",
    "tailwindcss": "^1.6.2",
    "webpack": "^4.44.1",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "author": "Miriam Gonzalez",
  "license": "MIT",
  "dependencies": {}
}
