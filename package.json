{
  "name": "stencil-boilerplate",
  "version": "0.0.1",
  "description": "Stencil boilerplate for Web components",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-boilerplate/stencil-boilerplate.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "build-storybook": "build-storybook",
    "dev": "stencil build --watch --serve",
    "generate": "stencil generate",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "serve": "serve www -p 5000",
    "start": "run-p dev storybook",
    "storybook": "start-storybook -p 6006",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@stencil/core": "1.17.2",
    "@stencil/eslint-plugin": "^0.3.1",
    "@storybook/addon-essentials": "^6.0.0-rc.26",
    "@storybook/web-components": "^6.0.0-rc.26",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.6.0",
    "eslint-plugin-react": "^7.20.5",
    "jest": "^26.2.2",
    "lit-html": "^1.2.1",
    "npm-run-all": "^4.1.5"
  },
  "author": "Miriam Gonzalez",
  "license": "MIT",
  "dependencies": {}
}
